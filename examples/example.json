{
    "Idle": [
        {
            "trigger": "StartWarmup",
            "condition": "hasGamePiece()",
            "destination": "WarmUp"
        }
    ],
    "WarmUp": [
        {
            "trigger": "CancelWarmup",
            "destination": "Idle"
